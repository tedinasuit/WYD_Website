cript>\nimport { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\n\nexport default {\n  name: 'HomeScreen',\n  components: {\n    SloganText\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n    });\n\n    const showNavBar = ref(false); // Define showNavBar here\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n      if (state.isLogoSmall) {\n        showNavBar.value = true;\n      }\n    };\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out'\n    }));\n\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n    }));\n\n    return {\n      state,\n      showNavBar, // Return showNavBar from the setup function\n      toggleLogoSize,\n      logoStyle,\n      containerStyle\n    };\n  }\n};\n</script>\n\n\n<style scoped>\n.container {\n  text-align: center;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.image {\n  max-width: 100%;\n  height: auto;\n  cursor: pointer;\n}\n\n.slogan {\n  font-family: \"League Spartan\", sans-serif;\n  font-weight: 900;\n  font-style: normal;\n  font-size: 10vh;\n  color: white;\n  margin-top: 9vh;\n}\n</style>\n"],"mappings":"AAQA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,OAAOC,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVF,UAAS,EAATA;EACF,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,WAAW,EAAE;IACf,CAAC,CAAC;IAEF,IAAMC,UAAS,GAAIC,GAAG,CAAC,KAAK,CAAC,EAAE;;IAE/B,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BJ,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;MACtC,IAAID,KAAK,CAACC,WAAW,EAAE;QAC