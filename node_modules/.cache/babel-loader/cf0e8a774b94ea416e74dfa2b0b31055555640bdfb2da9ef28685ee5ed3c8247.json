{"ast":null,"code":"import { reactive, computed, onMounted, watch } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport InstagramButton from '@/components/InstagramButton.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText: SloganText,\n    InstagramButton: InstagramButton,\n    NavBar: NavBar // Register the NavBar component\n  },\n  setup: function setup() {\n    var state = reactive({\n      isLogoSmall: false,\n      showNavBar: false\n    });\n    var toggleLogoSize = function toggleLogoSize() {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n    var logoStyle = computed(function () {\n      return {\n        maxWidth: state.isLogoSmall ? '160px' : '100%',\n        transition: 'max-width 0.5s ease-in-out'\n      };\n    });\n    var containerStyle = computed(function () {\n      return {\n        textAlign: 'center',\n        position: 'absolute',\n        top: state.isLogoSmall ? '10px' : '50%',\n        left: '50%',\n        transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n        transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n      };\n    });\n\n    // Computed property for background image\n    var backgroundImage = computed(function () {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n\n    // Data property to track whether text is above logo\n    var textAboveLogo = ref(false);\n\n    // Watcher to monitor the value of textAboveLogo\n    watch(textAboveLogo, function (newValue) {\n      state.showNavBar = newValue; // Update showNavBar based on textAboveLogo\n    });\n\n    // Set the document title\n    onMounted(function () {\n      document.title = 'What You Design';\n    });\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoStyle: logoStyle,\n      containerStyle: containerStyle,\n      backgroundImage: backgroundImage,\n      textAboveLogo: textAboveLogo\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","onMounted","watch","SloganText","InstagramButton","NavBar","name","components","setup","state","isLogoSmall","showNavBar","toggleLogoSize","logoStyle","maxWidth","transition","containerStyle","textAlign","position","top","left","transform","backgroundImage","require","textAboveLogo","ref","newValue","document","title"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"background\" :style=\"{ 'background-image': `url(${backgroundImage})` }\"></div>\n    <div class=\"content-wrapper\">\n      <Transition>\n        <router-view v-if=\"state.isLogoSmall\"></router-view> <!-- Apply fade transition -->\n      </Transition>\n      <div class=\"container\" :style=\"containerStyle\">\n        <img class=\"logoimage\" src=\"@/assets/WhatYouDesign-TransWit.png\" :style=\"logoStyle\" alt=\"WYDLogo\" @click=\"toggleLogoSize\">\n        <SloganText v-if=\"!state.isLogoSmall\" />\n      </div>\n      <NavBar v-if=\"state.showNavBar\" :class=\"{ 'black-background': textAboveLogo }\" /> <!-- Add dynamic class based on textAboveLogo -->\n    </div>\n    <Transition>\n      <InstagramButton v-if=\"state.showNavBar\" />\n    </Transition>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed, onMounted, watch } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport InstagramButton from '@/components/InstagramButton.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText,\n    InstagramButton,\n    NavBar // Register the NavBar component\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n      showNavBar: false\n    });\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out'\n    }));\n\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n    }));\n\n    // Computed property for background image\n    const backgroundImage = computed(() => {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n\n    // Data property to track whether text is above logo\n    const textAboveLogo = ref(false);\n\n    // Watcher to monitor the value of textAboveLogo\n    watch(textAboveLogo, (newValue) => {\n      state.showNavBar = newValue; // Update showNavBar based on textAboveLogo\n    });\n\n    // Set the document title\n    onMounted(() => {\n      document.title = 'What You Design';\n    });\n\n    return {\n      state,\n      toggleLogoSize,\n      logoStyle,\n      containerStyle,\n      backgroundImage,\n      textAboveLogo\n    };\n  }\n};\n</script>\n\n<style>\n// Existing styles...\n\n.black-background {\n  background-color: black; // Change background color to black when this class is applied\n}\n</style>\n"],"mappings":"AAoBA,SAASA,QAAQ,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,KAAI,QAAS,KAAK;AAC1D,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,MAAK,MAAO,yBAAyB,EAAE;;AAE9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,UAAU,EAAVA,UAAU;IACVC,eAAe,EAAfA,eAAe;IACfC,MAAK,EAALA,MAAK,CAAE;EACT,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIV,QAAQ,CAAC;MACrBW,W