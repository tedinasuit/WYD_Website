{"ast":null,"code":"import { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport NavBar from '@/components/NavBar.vue';\nexport default {\n  name: 'HomeScreen',\n  components: {\n    SloganText: SloganText,\n    NavBar: NavBar\n  },\n  setup: function setup() {\n    var state = reactive({\n      isLogoSmall: false\n    });\n    var toggleLogoSize = function toggleLogoSize() {\n      state.isLogoSmall = !state.isLogoSmall;\n    };\n    var logoStyle = computed(function () {\n      return {\n        maxWidth: state.isLogoSmall ? '160px' : '100%',\n        transition: 'max-width 0.5s ease-in-out',\n        zIndex: state.isLogoSmall ? '2' : 'auto' // Ensure logo stays on top when small\n      };\n    });\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoStyle: logoStyle\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","SloganText","NavBar","name","components","setup","state","isLogoSmall","toggleLogoSize","logoStyle","maxWidth","transition","zIndex"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/components/HomeScreen.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <img class=\"image\" src=\"@/assets/WhatYouDesign-TransWit.png\" :style=\"logoStyle\" alt=\"WYDLogo\" @click=\"toggleLogoSize\">\n    <div class=\"content\">\n      <SloganText v-if=\"!state.isLogoSmall\" />\n      <NavBar v-if=\"state.isLogoSmall\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport NavBar from '@/components/NavBar.vue';\n\nexport default {\n  name: 'HomeScreen',\n  components: {\n    SloganText,\n    NavBar\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n    });\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n    };\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out',\n      zIndex: state.isLogoSmall ? '2' : 'auto' // Ensure logo stays on top when small\n    }));\n\n    return {\n      state,\n      toggleLogoSize,\n      logoStyle\n    };\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  position: relative;\n  width: 100%;\n  height: 100vh;\n}\n\n.image {\n  max-width: 100%;\n  height: auto;\n  cursor: pointer;\n}\n\n.content {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  text-align: center;\n}\n\n</style>\n"],"mappings":"AAWA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVH,UAAU,EAAVA,UAAU;IACVC,MAAK,EAALA;EACF,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIP,QAAQ,CAAC;MACrBQ,WAAW,EAAE;IACf,CAAC,CAAC;IAEF,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BF,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;IACxC,CAAC;IAED,IAAME,SAAQ,GAAIT,QAAQ,CAAC;MAAA,OAAO;QAChCU,QAAQ,EAAEJ,KAAK,CAACC,WAAU,GAAI,OAAM,GAAI,MAAM;QAC9CI,UAAU,EAAE,4BAA4B;QACxCC,MAAM,EAAEN,KAAK,CAACC,WAAU,GAAI,GAAE,GAAI,MAAK,CAAE;MAC3C,CAAC;IAAA,CAAC,CAAC;IAEH,OAAO;MACLD,KAAK,EAALA,KAAK;MACLE,cAAc,EAAdA,cAAc;MACdC,SAAQ,EAARA;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}