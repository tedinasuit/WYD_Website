{"ast":null,"code":"import { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText: SloganText,\n    NavBar: NavBar // Register the NavBar component\n  },\n  setup: function setup() {\n    var state = reactive({\n      isLogoSmall: false,\n      showNavBar: false\n    });\n    var toggleLogoSize = function toggleLogoSize() {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n    var logoStyle = computed(function () {\n      return {\n        maxWidth: state.isLogoSmall ? '160px' : '100%',\n        transition: 'max-width 0.5s ease-in-out'\n      };\n    });\n    var containerStyle = computed(function () {\n      return {\n        textAlign: 'center',\n        position: 'absolute',\n        top: state.isLogoSmall ? '10px' : '50%',\n        left: '50%',\n        transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n        transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n      };\n    });\n\n    // Computed property for background image\n    var backgroundImage = computed(function () {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoStyle: logoStyle,\n      containerStyle: containerStyle,\n      backgroundImage: backgroundImage\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","SloganText","NavBar","name","components","setup","state","isLogoSmall","showNavBar","toggleLogoSize","logoStyle","maxWidth","transition","containerStyle","textAlign","position","top","left","transform","backgroundImage","require"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"background\" :style=\"{ 'background-image': `url(${backgroundImage})` }\">\n    <NavBar v-if=\"state.showNavBar\" /> <!-- Render NavBar only if showNavBar is true -->\n    <div class=\"container\" :style=\"containerStyle\">\n      <img class=\"image\" src=\"@/assets/WhatYouDesign-TransWit.png\" :style=\"logoStyle\" alt=\"WYDLogo\" @click=\"toggleLogoSize\">\n      <SloganText v-if=\"!state.isLogoSmall\" />\n    </div>\n    <Transition>\n      <router-view v-if=\"state.isLogoSmall\" ></router-view> <!-- Apply fade transition -->\n    </Transition>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText,\n    NavBar // Register the NavBar component\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n      showNavBar: false\n    });\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out'\n    }));\n\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n    }));\n\n    // Computed property for background image\n    const backgroundImage = computed(() => {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n\n    return {\n      state,\n      toggleLogoSize,\n      logoStyle,\n      containerStyle,\n      backgroundImage\n    };\n  }\n};\n</script>\n\n<style>\nbody {\n}\n\n.background {\n  position: fixed; /* Ensures background covers entire viewport */\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%; /* Fills the entire viewport */\n  background-size: cover; /* Scales image to cover the entire container */\n  background-position: center; /* Centers the image within the container */\n  transition: background-image 0.5s ease; /* Apply transition to background image */\n}\n  \n.v-enter-active {\n  transition: opacity 0.3s ease;\n  transition-delay: 0.1s; /* Add a delay of 0.3s before the fade-in transition */\n}\n\n.v-enter-from {\n  opacity: 0;\n}\n</style>\n\n<style scoped>\n.container {\n  text-align: center;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.image {\n  max-width: 100%;\n  height: auto;\n  cursor: pointer;\n  z-index: 2;\n}\n\n</style>\n"],"mappings":"AAcA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,MAAK,MAAO,yBAAyB,EAAE;;AAE9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVH,UAAU,EAAVA,UAAU;IACVC,MAAK,EAALA,MAAK,CAAE;EACT,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIP,QAAQ,CAAC;MACrBQ,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE;IACd,CAAC,CAAC;IAEF,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BH,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;MACtCD,KAAK,CAACE,UAAS,GAAIF,KAAK,CAACC,WAAW,EAAE;IACxC,CAAC;IAED,IAAMG,SAAQ,GAAIV,QAAQ,CAAC;MAAA,OAAO;QAChCW,QAAQ,EAAEL,KAAK,CAACC,WAAU,GAAI,OAAM,GAAI,MAAM;QAC9CK,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;IAEH,IAAMC,cAAa,GAAIb,QAAQ,CAAC;MAAA,OAAO;QACrCc,SAAS,EAAE,QAAQ;QACnBC,QAAQ,EAAE,UAAU;QACpBC,GAAG,EAAEV,KAAK,CAACC,WAAU,GAAI,MAAK,GAAI,KAAK;QACvCU,IAAI,EAAE,KAAK;QACXC,SAAS,EAAEZ,KAAK,CAACC,WAAU,GAAI,oBAAmB,GAAI,uBAAuB;QAC7EK,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;;IAEH;IACA,IAAMO,eAAc,GAAInB,QAAQ,CAAC,YAAM;MACrC,OAAOM,KAAK,CAACC,WAAU,GAAIa,OAAO,CAAC,mCAAmC,IAAIA,OAAO,CAAC,+BAA+B,CAAC;IACpH,CAAC,CAAC;IAEF,OAAO;MACLd,KAAK,EA