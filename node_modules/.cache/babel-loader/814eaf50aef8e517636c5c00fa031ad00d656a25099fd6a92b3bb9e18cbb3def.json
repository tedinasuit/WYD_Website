     document.head.appendChild(link);\n      window.addEventListener('scroll', handleScroll);\n    });\n    var handleScroll = function handleScroll() {\n      var containerTop = containerRef.value.getBoundingClientRect().top;\n      if (containerTop <= window.innerHeight * 0.05) {\n        // Check if top of container is within 5vh from top\n        toggleLogoSize();\n      }\n    };\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoSrc: logoSrc,\n      logoStyle: logoStyle,\n      containerStyle: containerStyle,\n      backgroundImage: backgroundImage,\n      containerRef: containerRef\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","onMounted","ref","SloganText","InstagramButton","NavBar","name","components","setup","state","isLogoSmall","showNavBar","toggleLogoSize","containerStyle","textAlign","position","top","left","transform","transition","logoSrc","require","logoStyle","maxWidth","backgroundImage","containerRef","document","title","link","createElement","rel","href","as","head","appendChild","window","addEventListener","handleScroll","containerTop","value","getBoundingClientRect","innerHeight"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"background\" :style=\"{ 'background-image': `url(${backgroundImage})` }\"></div>\n    <div class=\"content-wrapper\">\n      <Transition>\n        <router-view v-if=\"state.isLogoSmall\"></router-view> <!-- Apply fade transition -->\n      </Transition>\n      <div class=\"container\" :style=\"containerStyle\" ref=\"container\">\n        <img class=\"logoimage\" :src=\"logoSrc\" :style=\"logoStyle\" alt=\"WYDLogo\" @click=\"toggleLogoSize\">\n        <SloganText v-if=\"!state.isLogoSmall\" />\n      </div>\n      <NavBar style=\"z-index: 4;\" v-if=\"state.showNavBar\" /> <!-- Render NavBar only if showNavBar is true -->\n    </div>\n    <Transition>\n      <InstagramButton v-if=\"state.showNavBar\"  />\n    </Transition>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed, onMounted, ref } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport InstagramButton from '@/components/InstagramButton.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText,\n    InstagramButton,\n    NavBar // Register the NavBar component\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n      showNavBar: false\n    });\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n    }));\n\n    const logoSrc = computed(() => {\n      return state.isLogoSmall ? require('@/assets/WhatYouDesign-Wit.png') : require('@/assets/WhatYouDesign-TransWit.png');\n    });\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out'\n    }));\n\n    const backgroundImage = computed(() => {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n\n    const containerRef = ref(null);\n\n    onMounted(() => {\n      document.title = 'What You Design';\n      const link = document.createElement('link');\n      link.rel = 'preload';\n      link.href = require('@/assets/hippie-achtergrond-2.png'); // Path to your background image\n      link.as = 'image';\n      document.head.appendChild(link);\n\n      window.addEventListener('scroll', handleScroll);\n    });\n\n    const handleScroll = () => {\n      const containerTop = containerRef.value.getBoundingClientRect().top;\n      if (containerTop <= window.innerHeight * 0.05) { // Check if top of container is within 5vh from top\n        toggleLogoSize();\n      }\n    };\n\n    return {\n      state,\n      toggleLogoSize,\n      logoSrc,\n      logoStyle,\n      containerStyle,\n      backgroundImage,\n      containerRef\n    };\n  }\n};\n</script>\n\n<style>\n/* Your existing styles */\n\n.logoimage {\n  max-width: 100%;\n  height: auto;\n  cursor: pointer;\n  animation: changeLogoSrc 0.5s forwards;\n}\n\n@keyframes changeLogoSrc {\n  from { opacity: 1; }\n  to { opacity: 0; }\n}\n\n</style>\n"],"mappings":"AAoBA,SAASA,QAAQ,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,GAAE,QAAS,KAAK;AACxD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,MAAK,MAAO,yBAAyB,EAAE;;AAE9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,UAAU,EAAVA,UAAU;IACVC,eAAe,EAAfA,eAAe;IACfC,MAAK,EAALA,MAAK,CAAE;EACT,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIV,QAAQ,CAAC;MACrBW,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE;IACd,CAAC,CAAC;IAEF,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BH,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;MACtCD,KAAK,CAACE,UAAS,GAAIF,KAAK,CAACC,WAAW,EAAE;IACxC,CAAC;IAED,IAAMG,cAAa,GAAIb,QAAQ,CAAC;MAAA,OAAO;QACrCc,SAAS,EAAE,QAAQ;QACnBC,QAAQ,EAAE,UAAU;QACpBC,GAAG,EAAEP,KAAK,CAACC,WAAU,GAAI,MAAK,GAAI,KAAK;QACvCO,IAAI,EAAE,KAAK;QACXC,SAAS,EAAET,KAAK,CAACC,WAAU,GAAI,oBAAmB,GAAI,uBAAuB;QAC7ES,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;IAEH,IAAMC,OAAM,GAAIpB,QAAQ,CAAC,YAAM;MAC7B,OAAOS,KAAK,CAACC,WAAU,GAAIW,OAAO,CAAC,gCAAgC,IAAIA,OAAO,CAAC,qCAAqC,CAAC;IACvH,CAAC,CAAC;IAEF,IAAMC,SAAQ,GAAItB,QAAQ,CAAC;MAAA,OAAO;QAChCuB,QAAQ,EAAEd,KAAK,CAACC,WAAU,GAAI,OAAM,GAAI,MAAM;QAC9CS,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;IAEH,IAAMK,eAAc,GAAIxB,QAAQ,CAAC,YAAM;MACrC,OAAOS,KAAK,CAACC,WAAU,GAAIW,OAAO,CAAC,mCAAmC,IAAIA,OAAO,CAAC,+BAA+B,CAAC;IACpH,CAAC,CAAC;IAEF,IAAMI,YAAW,GAAIvB,GAAG,CAAC,IAAI,CAAC;IAE9B