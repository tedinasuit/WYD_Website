{"ast":null,"code":"import { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText: SloganText,\n    NavBar: NavBar // Register the NavBar component\n  },\n  setup: function setup() {\n    var state = reactive({\n      isLogoSmall: false,\n      showNavBar: false,\n      backgroundImage: '@/assets/Hippie-kleurrijk.png' // Initial background image\n    });\n    var toggleLogoSize = function toggleLogoSize() {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar only when logo is small\n      state.backgroundImage = state.isLogoSmall ? '@/assets/Hippie-achtergrond-2.png' : '@/assets/Hippie-kleurrijk.png'; // Toggle background image\n    };\n    var logoStyle = computed(function () {\n      return {\n        maxWidth: state.isLogoSmall ? '160px' : '100%',\n        transition: 'max-width 0.5s ease-in-out'\n      };\n    });\n    var containerStyle = computed(function () {\n      return {\n        textAlign: 'center',\n        position: 'absolute',\n        top: state.isLogoSmall ? '10px' : '50%',\n        left: '50%',\n        transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n        transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n      };\n    });\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoStyle: logoStyle,\n      containerStyle: containerStyle\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","SloganText","NavBar","name","components","setup","state","isLogoSmall","showNavBar","backgroundImage","toggleLogoSize","logoStyle","maxWidth","transition","containerStyle","textAlign","position","top","left","transform"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"background\">\n    <NavBar v-if=\"state.showNavBar\" /> <!-- Render NavBar only if showNavBar is true -->\n    <div class=\"container\" :style=\"containerStyle\">\n      <img class=\"image\" src=\"@/assets/WhatYouDesign-TransWit.png\" :style=\"logoStyle\" alt=\"WYDLogo\" @click=\"toggleLogoSize\">\n      <SloganText v-if=\"!state.isLogoSmall\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText,\n    NavBar // Register the NavBar component\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n      showNavBar: false,\n      backgroundImage: '@/assets/Hippie-kleurrijk.png' // Initial background image\n    });\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar only when logo is small\n      state.backgroundImage = state.isLogoSmall ? '@/assets/Hippie-achtergrond-2.png' : '@/assets/Hippie-kleurrijk.png'; // Toggle background image\n    };\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out'\n    }));\n\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n    }));\n\n    return {\n      state,\n      toggleLogoSize,\n      logoStyle,\n      containerStyle\n    };\n  }\n};\n</script>\n\n<style>\n.background {\n  position: absolute; /* Ensures background covers entire viewport */\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%; /* Fills the entire viewport */\n  background-size: cover; /* Scales image to cover the entire container */\n  background-position: center; /* Centers the image within the container */\n  transition: background-image 0.5s ease-in-out; /* Smooth transition for background image */\n}\n\n</style>\n\n<style scoped>\n.container {\n  text-align: center;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.image {\n  max-width: 100%;\n  height: auto;\n  cursor: pointer;\n  z-index: 2;\n}\n\n</style>\n"],"mappings":"AAWA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,MAAK,MAAO,yBAAyB,EAAE;;AAE9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVH,UAAU,EAAVA,UAAU;IACVC,MAAK,EAALA,MAAK,CAAE;EACT,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIP,QAAQ,CAAC;MACrBQ,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE,KAAK;MACjBC,eAAe,EAAE,+BAA8B,CAAE;IACnD,CAAC,CAAC;IAEF,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BJ,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;MACtCD,KAAK,CAACE,UAAS,GAAIF,KAAK,CAACC,WAAW,EAAE;MACtCD,KAAK,CAACG,eAAc,GAAIH,KAAK,CAACC,WAAU,GAAI,mCAAkC,GAAI,+BAA+B,EAAE;IACrH,CAAC;IAED,IAAMI,SAAQ,GAAIX,QAAQ,CAAC;MAAA,OAAO;QAChCY,QAAQ,EAAEN,KAAK,CAACC,WAAU,GAAI,OAAM,GAAI,MAAM;QAC9CM,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;IAEH,IAAMC,cAAa,GAAId,QAAQ,CAAC;MAAA,OAAO;QACrCe,SAAS,EAAE,QAAQ;QACnBC,QAAQ,EAAE,UAAU;QACpBC,GAAG,EAAEX,KAAK,CAACC,WAAU,GAAI,MAAK,GAAI,KAAK;QACvCW,IAAI,EAAE,KAAK;QACXC,SAAS,EAAEb,KAAK,CAACC,WAAU,GAAI,oBAAmB,GAAI,uBAAuB;QAC7EM,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;IAEH,OAAO;MACLP,KAAK,EAALA,KAAK;MACLI,cAAc,EAAdA,cAAc;MACdC,SAAS,EAATA,SAAS;MACTG,cAAa,EAAbA;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}