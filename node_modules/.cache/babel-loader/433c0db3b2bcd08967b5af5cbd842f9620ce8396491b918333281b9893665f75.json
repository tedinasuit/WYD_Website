{"ast":null,"code":"import { reactive, computed, onMounted, onBeforeUnmount } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport InstagramButton from '@/components/InstagramButton.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText: SloganText,\n    InstagramButton: InstagramButton,\n    NavBar: NavBar // Register the NavBar component\n  },\n  setup: function setup() {\n    var state = reactive({\n      isLogoSmall: false,\n      showNavBar: false,\n      navbarBackground: false // Add a property to track the navbar background color\n    });\n    var toggleLogoSize = function toggleLogoSize() {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n    var logoStyle = computed(function () {\n      return {\n        maxWidth: state.isLogoSmall ? '160px' : '100%',\n        transition: 'max-width 0.5s ease-in-out'\n      };\n    });\n    var containerStyle = computed(function () {\n      return {\n        textAlign: 'center',\n        position: 'absolute',\n        top: state.isLogoSmall ? '10px' : '50%',\n        left: '50%',\n        transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n        transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n      };\n    });\n\n    // Computed property for background image\n    var backgroundImage = computed(function () {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n\n    // Function to handle scroll event\n    var handleScroll = function handleScroll() {\n      var firstComponent = document.getElementById('firstComponent');\n      var navbar = document.querySelector('.navbar');\n      if (firstComponent && navbar) {\n        var firstComponentRect = firstComponent.getBoundingClientRect();\n        var navbarRect = navbar.getBoundingClientRect();\n        if (firstComponentRect.bottom <= navbarRect.bottom) {\n          state.navbarBackground = true;\n        } else {\n          state.navbarBackground = false;\n        }\n      }\n    };\n\n    // Listen for scroll event\n    onMounted(function () {\n      window.addEventListener('scroll', handleScroll);\n    });\n\n    // Remove scroll event listener when component is unmounted\n    onBeforeUnmount(function () {\n      window.removeEventListener('scroll', handleScroll);\n    });\n\n    // Set the document title and preload background image\n    onMounted(function () {\n      document.title = 'What You Design';\n      var link = document.createElement('link');\n      link.rel = 'preload';\n      link.href = require('@/assets/hippie-achtergrond-2.png'); // Path to your background image\n      link.as = 'image';\n      document.head.appendChild(link);\n    });\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoStyle: logoStyle,\n      containerStyle: containerStyle,\n      backgroundImage: backgroundImage\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","onMounted","onBeforeUnmount","SloganText","InstagramButton","NavBar","name","components","setup","state","isLogoSmall","showNavBar","navbarBackground","toggleLogoSize","logoStyle","maxWidth","transition","containerStyle","textAlign","position","top","left","transform","backgroundImage","require","handleScroll","firstComponent","document","getElementById","navbar","querySelector","firstComponentRect","getBoundingClientRect","navbarRect","bottom","window","addEventListener","removeEventListener","title","link","createElement","rel","href","as","head","appendChild"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/App.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"background\" :style=\"{ 'background-image': `url(${backgroundImage})` }\"></div>\n    <div class=\"content-wrapper\">\n      <Transition>\n        <router-view v-if=\"state.isLogoSmall\" ></router-view> <!-- Apply fade transition -->\n      </Transition>\n      <div></div>\n      <div class=\"container\" :style=\"containerStyle\">\n        <img class=\"logoimage\" src=\"@/assets/WhatYouDesign-TransWit.png\" :style=\"logoStyle\" alt=\"WYDLogo\" @click=\"toggleLogoSize\">\n        <SloganText v-if=\"!state.isLogoSmall\" />\n      </div>\n      <NavBar :state=\"state\" v-if=\"state.showNavBar\" /> <!-- Apply 'scrolled' class dynamically -->\n    </div>\n    <Transition>\n      <InstagramButton v-if=\"state.showNavBar\"  />\n    </Transition>\n  </div>\n</template>\n\n<script>\nimport { reactive, computed, onMounted, onBeforeUnmount } from 'vue';\nimport SloganText from '@/components/SloganText.vue';\nimport InstagramButton from '@/components/InstagramButton.vue';\nimport NavBar from '@/components/NavBar.vue'; // Import the NavBar component\n\nexport default {\n  name: 'App',\n  components: {\n    SloganText,\n    InstagramButton,\n    NavBar // Register the NavBar component\n  },\n  setup() {\n    const state = reactive({\n      isLogoSmall: false,\n      showNavBar: false,\n      navbarBackground: false // Add a property to track the navbar background color\n    });\n\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n      state.showNavBar = state.isLogoSmall; // Show NavBar when the logo is small\n    };\n\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out'\n    }));\n\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out'\n    }));\n\n    // Computed property for background image\n    const backgroundImage = computed(() => {\n      return state.isLogoSmall ? require('@/assets/hippie-achtergrond-2.png') : require('@/assets/Hippie-kleurrijk.png');\n    });\n\n    // Function to handle scroll event\n    const handleScroll = () => {\n      const firstComponent = document.getElementById('firstComponent');\n      const navbar = document.querySelector('.navbar');\n\n      if (firstComponent && navbar) {\n        const firstComponentRect = firstComponent.getBoundingClientRect();\n        const navbarRect = navbar.getBoundingClientRect();\n\n        if (firstComponentRect.bottom <= navbarRect.bottom) {\n          state.navbarBackground = true;\n        } else {\n          state.navbarBackground = false;\n        }\n      }\n    };\n\n    // Listen for scroll event\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n    });\n\n    // Remove scroll event listener when component is unmounted\n    onBeforeUnmount(() => {\n      window.removeEventListener('scroll', handleScroll);\n    });\n\n    // Set the document title and preload background image\n    onMounted(() => {\n      document.title = 'What You Design';\n\n      const link = document.createElement('link');\n      link.rel = 'preload';\n      link.href = require('@/assets/hippie-achtergrond-2.png'); // Path to your background image\n      link.as = 'image';\n      document.head.appendChild(link);\n    });\n\n    return {\n      state,\n      toggleLogoSize,\n      logoStyle,\n      containerStyle,\n      backgroundImage\n    };\n  }\n};\n</script>\n\n\n<style>\nbody {\n  margin: 0;\n  padding: 0;\n  overflow: hidden; \n  background-size: cover;\n  background-position: center;\n  background-attachment: fixed; /* Keep the background image fixed */\n}\n\n.flexbox {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-wrap: wrap-reverse;\n  min-width: 90vw;\n}\n\n.title {\n    font-family: \"League Spartan\", sans-serif;\n    font-weight: 900;\n    font-style: normal;\n    color: white; /* Change the color of the slogan */\n    text-align: left;\n\n  }\n\n.text {\n    font-family: \"League Spartan\", sans-serif;\n    font-weight: 400;\n    font-style: normal;\n    color: white; /* Change the color of the slogan */\n    width: 400px;\n    max-width: 90vw;\n    text-align: left;\n}\n\n.text-wrap {\n  flex: 1; /* Allow text container to grow to fill remaining space */\n  max-width: 400px; /* Limit the maximum width of the text container */\n  padding: 5vw;\n  text-align: center; /* Center align text */\n}\n\n.title {\n  font-size: clamp(20px, 3rem, 8vw); /* Adjust title font size */\n}\n\n.text {\n  font-size: clamp(20px, 2rem, 6vw); /* Adjust text font size */\n  \n}\n\n.content-wrapper {\n  height: 100vh; /* Take the full height of the viewport */\n  overflow-y: auto; /* Enable vertical scrolling */\n}\n\n.container {\n  text-align: center;\n  position: relative; /* Change position to relative */\n  overflow-x: hidden;\n  z-index: 5;\n}\n\n.logoimage {\n  max-width: 100%;\n  height: auto;\n  cursor: pointer;\n}\n\n.image {\n  width: 65vh; /* Ensure image fills its container */\n  max-width: min(80vw, 60vh); /* Cap the max-width at either 80vw or 60vh, whichever is smaller */\n  object-fit: contain; /* Scale the image while preserving its aspect ratio */\n  border-radius: 10px;\n  filter: drop-shadow(1.5vw 0.8vw 7px #00000023);\n}\n\n.background {\n  position: absolute; /* Ensures background covers entire viewport */\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%; /* Fills the entire viewport */\n  background-size: cover; /* Scales image to cover the entire container */\n  background-position: center; /* Centers the image within the container */\n  transition: background-image 0.5s ease; /* Apply transition to background image */\n}\n\n.v-enter-active {\n  transition: opacity 0.3s ease;\n  transition-delay: 0.1s; /* Add a delay of 0.3s before the fade-in transition */\n}\n\n.v-enter-from {\n  opacity: 0;\n}\n\n@media (orientation: portrait) {\n  .title {\n    text-align: center;\n  }\n\n  .text {\n    text-align: center;\n\n  }\n}\n\n.scrolled {\n  background-color: black;\n}\n\n\n</style>"],"mappings":"AAqBA,SAASA,QAAQ,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,eAAc,QAAS,KAAK;AACpE,OAAOC,UAAS,MAAO,6BAA6B;AACpD,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,MAAK,MAAO,yBAAyB,EAAE;;AAE9C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,UAAU,EAAVA,UAAU;IACVC,eAAe,EAAfA,eAAe;IACfC,MAAK,EAALA,MAAK,CAAE;EACT,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,KAAI,GAAIV,QAAQ,CAAC;MACrBW,WAAW,EAAE,KAAK;MAClBC,UAAU,EAAE,KAAK;MACjBC,gBAAgB,EAAE,KAAI,CAAE;IAC1B,CAAC,CAAC;IAEF,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BJ,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;MACtCD,KAAK,CAACE,UAAS,GAAIF,KAAK,CAACC,WAAW,EAAE;IACxC,CAAC;IAED,IAAMI,SAAQ,GAAId,QAAQ,CAAC;MAAA,OAAO;QAChCe,QAAQ,EAAEN,KAAK,CAACC,WAAU,GAAI,OAAM,GAAI,MAAM;QAC9CM,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;IAEH,IAAMC,cAAa,GAAIjB,QAAQ,CAAC;MAAA,OAAO;QACrCkB,SAAS,EAAE,QAAQ;QACnBC,QAAQ,EAAE,UAAU;QACpBC,GAAG,EAAEX,KAAK,CAACC,WAAU,GAAI,MAAK,GAAI,KAAK;QACvCW,IAAI,EAAE,KAAK;QACXC,SAAS,EAAEb,KAAK,CAACC,WAAU,GAAI,oBAAmB,GAAI,uBAAuB;QAC7EM,UAAU,EAAE;MACd,CAAC;IAAA,CAAC,CAAC;;IAEH;IACA,IAAMO,eAAc,GAAIvB,QAAQ,CAAC,YAAM;MACrC,OAAOS,KAAK,CAACC,WAAU,GAAIc,OAAO,CAAC,mCAAmC,IAAIA,OAAO,CAAC,+BAA+B,CAAC;IACpH,CAAC,CAAC;;IAEF;IACA,IAAMC,YAAW,GAAI,SAAfA,YAAWA,CAAA,EAAU;MACzB,IAAMC,cAAa,GAAIC,QAAQ,CAACC,cAAc,CAAC,gBAAgB,CAAC;MAChE,IAAMC,MAAK,GAAIF,QAAQ,CAACG,aAAa,CAAC,SAAS,CAAC;MAEhD,IAAIJ,cAAa,IAAKG,MAAM,EAAE;QAC5B,IAAME,kBAAiB,GAAIL,cAAc,CAACM,qBAAqB,CAAC,CAAC;QACjE,IAAMC,UAAS,GAAIJ,MAAM,CAACG,qBAAqB,CAAC,CAAC;QAEjD,IAAID,kBAAkB,CAACG,MAAK,IAAKD,UAAU,CAACC,MAAM,EAAE;UAClDzB,KAAK,CAACG,gBAAe,GAAI,IAAI;QAC/B,OAAO;UACLH,KAAK,CAACG,gBAAe,GAAI,KAAK;QAChC;MACF;IACF,CAAC;;IAED;IACAX,SAAS,CAAC,YAAM;MACdkC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEX,YAAY,CAAC;IACjD,CAAC,CAAC;;IAEF;IACAvB,eAAe,CAAC,YAAM;MACpBiC,MAAM,CAACE,mBAAmB,CAAC,QAAQ,EAAEZ,YAAY,CAAC;IACpD,CAAC,CAAC;;IAEF;IACAxB,SAAS,CAAC,YAAM;MACd0B,QAAQ