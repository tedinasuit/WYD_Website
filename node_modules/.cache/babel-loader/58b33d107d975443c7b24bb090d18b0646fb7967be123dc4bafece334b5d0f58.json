{"ast":null,"code":"import { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue'; // Import the SloganText component\n\nexport default {\n  name: 'HomeScreen',\n  components: {\n    SloganText: SloganText // Register the SloganText component\n  },\n  setup: function setup() {\n    // Reactive state\n    var state = reactive({\n      isLogoSmall: false\n    });\n\n    // Method to toggle the logo size\n    var toggleLogoSize = function toggleLogoSize() {\n      state.isLogoSmall = !state.isLogoSmall;\n    };\n\n    // Computed property for logo style\n    var logoStyle = computed(function () {\n      return {\n        maxWidth: state.isLogoSmall ? '160px' : '100%',\n        transition: 'max-width 0.5s ease-in-out' // Add transition for smooth resizing\n      };\n    });\n\n    // Computed property for container style\n    var containerStyle = computed(function () {\n      return {\n        textAlign: 'center',\n        position: 'absolute',\n        top: state.isLogoSmall ? '10px' : '50%',\n        left: '50%',\n        transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n        transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out' // Add transition for smooth repositioning\n      };\n    });\n\n    // Return data, methods, and computed properties accessible in the template\n    return {\n      state: state,\n      toggleLogoSize: toggleLogoSize,\n      logoStyle: logoStyle,\n      containerStyle: containerStyle\n    };\n  }\n};","map":{"version":3,"names":["reactive","computed","SloganText","name","components","setup","state","isLogoSmall","toggleLogoSize","logoStyle","maxWidth","transition","containerStyle","textAlign","position","top","left","transform"],"sources":["/Users/larshoeijmans/Documents/GitHub/WYD_Website/src/components/HomeScreen.vue"],"sourcesContent":["<template>\n    <div class=\"container\" :style=\"containerStyle\">\n      <img class=\"image\" src=\"@/assets/WhatYouDesign-TransWit.png\" :style=\"logoStyle\" alt=\"WYDLogo\">\n      <SloganText />\n      <button @click=\"toggleLogoSize\">Toggle Logo Size</button>\n    </div>\n</template>\n\n<script>\nimport { reactive, computed } from 'vue';\nimport SloganText from '@/components/SloganText.vue'; // Import the SloganText component\n\nexport default {\n  name: 'HomeScreen',\n  components: {\n    SloganText // Register the SloganText component\n  },\n  setup() {\n    // Reactive state\n    const state = reactive({\n      isLogoSmall: false\n    });\n\n    // Method to toggle the logo size\n    const toggleLogoSize = () => {\n      state.isLogoSmall = !state.isLogoSmall;\n    };\n\n    // Computed property for logo style\n    const logoStyle = computed(() => ({\n      maxWidth: state.isLogoSmall ? '160px' : '100%',\n      transition: 'max-width 0.5s ease-in-out' // Add transition for smooth resizing\n    }));\n\n    // Computed property for container style\n    const containerStyle = computed(() => ({\n      textAlign: 'center',\n      position: 'absolute',\n      top: state.isLogoSmall ? '10px' : '50%',\n      left: '50%',\n      transform: state.isLogoSmall ? 'translate(-50%, 0)' : 'translate(-50%, -50%)',\n      transition: 'top 0.5s ease-in-out, transform 0.5s ease-in-out' // Add transition for smooth repositioning\n    }));\n\n    // Return data, methods, and computed properties accessible in the template\n    return {\n      state,\n      toggleLogoSize,\n      logoStyle,\n      containerStyle\n    };\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  text-align: center;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n}\n\n.image {\n  max-width: 100%;\n  height: auto;\n}\n\n.slogan {\n  font-family: \"League Spartan\", sans-serif;\n  font-weight: 900;\n  font-style: normal;\n  font-size: 10vh; /* Adjust the font size as needed */\n  color: white; /* Change the color of the slogan */\n  margin-top: 9vh; /* Add some space between the image and the slogan */\n}\n.button {\n  margin-top: 20px;\n}\n</style>\n"],"mappings":"AASA,SAASA,QAAQ,EAAEC,QAAO,QAAS,KAAK;AACxC,OAAOC,UAAS,MAAO,6BAA6B,EAAE;;AAEtD,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVF,UAAS,EAATA,UAAS,CAAE;EACb,CAAC;EACDG,KAAK,WAAAA,MAAA,EAAG;IACN;IACA,IAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,WAAW,EAAE;IACf,CAAC,CAAC;;IAEF;IACA,IAAMC,cAAa,GAAI,SAAjBA,cAAaA,CAAA,EAAU;MAC3BF,KAAK,CAACC,WAAU,GAAI,CAACD,KAAK,CAACC,WAAW;IACxC,CAAC;;IAED;IACA,IAAME,SAAQ,GAAIR,QAAQ,CAAC;MAAA,OAAO;QAChCS,QAAQ,EAAEJ,KAAK,CAACC,WAAU,GAAI,OAAM,GAAI,MAAM;QAC9CI,UAAU,EAAE,4BAA2B,CAAE;MAC3C,CAAC;IAAA,CAAC,CAAC;;IAEH;IACA,IAAMC,cAAa,GAAIX,QAAQ,CAAC;MAAA,OAAO;QACrCY,SAAS,EAAE,QAAQ;QACnBC,QAAQ,EAAE,UAAU;QACpBC,GAAG,EAAET,KAAK,CAACC,WAAU,GAAI,MAAK,GAAI,KAAK;QACvCS,IAAI,EAAE,KAAK;QACXC,SAAS,EAAEX,KAAK,CAACC,WAAU,GAAI,oBAAmB,GAAI,uBAAuB;QAC7EI,UAAU,EAAE,kDAAiD,CAAE;MACjE,CAAC;IAAA,CAAC,CAAC;;IAEH;IACA,OAAO;MACLL,KAAK,EAALA,KAAK;MACLE,cAAc,EAAdA,cAAc;MACdC,SAAS,EAATA,SAAS;MACTG,cAAa,EAAbA;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}